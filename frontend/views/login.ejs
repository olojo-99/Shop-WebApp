<html>

    <head>
			<title><%= title %></title>
    </head>

    <body>
        <h1>Login</h1>
        <form id="login-form"> 
            <label for="username-input">Username</label>
            <input type="text" name="username-input" id="username" placeholder="Your username">
            <label for="password-input">Password</label>
            <input type="password" id="password" name="password-input" placeholder="Your password">
            <button type="submit">Login</button>
        </form>
		
        <script src="/javascripts/formvalidator.js"></script>

    </body>
</html>